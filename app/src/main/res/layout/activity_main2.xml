<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main2Activity">


    <LinearLayout
        android:id="@+id/l1"
        android:layout_width="332dp"
        android:layout_height="92dp"
        android:layout_marginTop="88dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/t">

        <ImageView
            android:id="@+id/img"
            android:layout_width="109dp"
            android:layout_height="85dp"
            android:onClick="cambio"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/img2"
            android:layout_width="109dp"
            android:layout_height="85dp"
            android:onClick="cambio2"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/img3"
            android:layout_width="109dp"
            android:layout_height="85dp"
            android:onClick="cambio3"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="MissingConstraints" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/l2"
        android:layout_width="337dp"
        android:layout_height="90dp"
        android:layout_marginTop="36dp"
        android:layout_weight="92"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/l1">

        <ImageView
            android:id="@+id/img4"
            android:layout_width="109dp"
            android:layout_height="85dp"
            android:onClick="cambio4"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/img5"
            android:layout_width="109dp"
            android:layout_height="85dp"
            android:onClick="cambio5"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/img6"
            android:layout_width="109dp"
            android:layout_height="85dp"
            android:onClick="cambio6"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="MissingConstraints" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/l3"
        android:layout_width="337dp"
        android:layout_height="90dp"
        android:layout_marginTop="36dp"
        android:layout_weight="92"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/l2"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/img7"
            android:layout_width="109dp"
            android:layout_height="85dp"
            android:onClick="cambio7"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/img8"
            android:layout_width="109dp"
            android:layout_height="85dp"
            android:onClick="cambio8"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/img9"
            android:layout_width="109dp"
            android:layout_height="85dp"
            android:onClick="cambio9"
            app:srcCompat="@drawable/ic_launcher_background"
            tools:ignore="MissingConstraints" />
    </LinearLayout>

    <TextView
        android:id="@+id/username"
        android:layout_width="61dp"
        android:layout_height="51dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="64dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/username2"
        android:layout_width="61dp"
        android:layout_height="51dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="100dp"
        android:layout_marginRight="100dp"
        android:text="VS"
        app:layout_constraintEnd_toStartOf="@+id/username2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/t"
        android:layout_width="103dp"
        android:layout_height="47dp"
        android:layout_marginTop="172dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.581"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />




</androidx.constraintlayout.widget.ConstraintLayout>